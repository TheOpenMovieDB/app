{
    "plugins": [
        "prettier-plugin-blade"
    ],
    "overrides": [
        {
            "files": [
                "*.blade.php"
            ],
            "options": {
                "parser": "blade"
            }
        }
    ],
    "printWidth": 100,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true
}
